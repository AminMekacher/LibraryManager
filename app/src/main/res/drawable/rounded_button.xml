<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape ="rectangle" >

            <stroke
                android:height=".0dip"
                android:width="1.0dip"
                android:color="@color/colorPrimaryDark" />

            <solid android:color="@color/colorPrimaryDark" />

            <corners
                android:bottomLeftRadius="12.0dip"
                android:bottomRightRadius="12.0dip"
                android:radius="12.0dip"
                android:topLeftRadius="12.0dip"
                android:topRightRadius="12.0dip" />
        </shape>
    </item>

    <item android:state_pressed="true" >
        <shape android:shape ="rectangle" >

            <stroke
                android:height=".0dip"
                android:width="1.0dip"
                android:color="@color/greenCalendar" />

            <solid android:color="@color/greenCalendar" />

            <corners
                android:bottomLeftRadius="12.0dip"
                android:bottomRightRadius="12.0dip"
                android:radius="12.0dip"
                android:topLeftRadius="12.0dip"
                android:topRightRadius="12.0dip" />
        </shape>


    </item>

</selector>